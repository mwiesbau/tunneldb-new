<h1>Listing tunneling Under</h1>
<br>

<table class= "list">
  <tr>
	<th></th>
    <th>Name</th>
    </tr>

<% for tunneling_under in @tunneling_unders %>



 		 <tr class="<%= alternate %>">		
			<td width ="60px">
              <%= link_to (image_tag("list-icons/icon-view-off.png", :border => 0, :mouseover => "list-icons/icon-view-on.png"), :action =>'show', :id => tunneling_under.id) %>
              <%= link_to (image_tag("list-icons/icon-edit-off.png", :border => 0, :mouseover => "list-icons/icon-edit-on.png"), :action =>'edit', :id => tunneling_under.id) %>
              <%= link_to (image_tag("list-icons/icon-delete-off.png", :border => 0, :mouseover => "list-icons/icon-delete-on.png"), tunneling_under, :confirm => 'Are you sure?', :method => :delete) %>
            </td>
		
        
            <td><%=h tunneling_under.name %></td>
          </tr>
 
<% end %>
</table>

<br />

<%= link_to 'New tunneling_under', new_tunneling_under_path %>