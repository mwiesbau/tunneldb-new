    <title>Log in</title>  
      
    <% form_for @user_session do |form| %>  
    <%= form.error_messages %>  
    <ol class="formList">  
      <p>  
        <%= form.label :username, "Username" %>  
        <%= form.text_field :username %>  
      </p>  
     <p>  
       <%= form.label :password, "Password" %>  
       <%= form.password_field :password %>  
     </p>  
     <p>  
       <%= form.submit "Submit" %>  
     </p>  
   </ol>  
   <% end %>  