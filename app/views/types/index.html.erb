<h1>Listing Project Types</h1>
<br>

<table class= "list">
  <tr>
	<th></th>
    <th>Name</th>
    </tr>

<% for type in @types %>



 		 <tr class="<%= alternate %>">		
			<td width ="60px">
              <%= link_to (image_tag("list-icons/icon-view-off.png", :border => 0, :mouseover => "list-icons/icon-view-on.png"), :action =>'show', :id => type.id) %>
              <%= link_to (image_tag("list-icons/icon-edit-off.png", :border => 0, :mouseover => "list-icons/icon-edit-on.png"), :action =>'edit', :id => type.id) %>
              <%= link_to (image_tag("list-icons/icon-delete-off.png", :border => 0, :mouseover => "list-icons/icon-delete-on.png"), type, :confirm => 'Are you sure?', :method => :delete) %>
            </td>
		
        
            <td><%=h type.name %></td>
          </tr>
 
<% end %>
</table>

<br />

<%= link_to 'New type', new_type_path %>