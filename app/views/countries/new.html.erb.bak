<h1>New country</h1>

<%= error_messages_for :country %>

<% form_for(@country) do |f| %>
  <p>
    <b>Symbol</b><br />
    <%= f.text_field :symbol %>
  </p>

  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', countries_path %>
